{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
      
    ],
    "rules": {
        "no-console": 1,
        "no-loops/no-loops": 2,
        "import/no-extraneous-dependencies": [2,  {"devDependencies": true} ]
    }
  }